<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE issue-xml PUBLIC "-//Atypon//DTD Atypon Systems Journal Archiving and Interchange NLM DTD v3.0.2 20101108//EN" "issuexml.dtd">
<issue-xml>
	<journal-meta>
		<journal-id journal-id-type="publisher-id">{{journal_publisher_id }}</journal-id>
		<journal-id journal-id-type="journal-code">{{ journal_code }}</journal-id>
		<journal-id journal-id-type="doi-code">{{ doi_code }}</journal-id>
		<journal-title-group>
			<journal-title>Journal of Digital History</journal-title>
		</journal-title-group>
		<issn pub-type="epub">{{ issn }}</issn>
		<publisher>
			<publisher-name>De Gruyter</publisher-name>
		</publisher>
	</journal-meta>
	<issue-meta>
		<pub-date pub-type="ppub">
			<day>{{ issueXml.issue_date|date:"d"}}</day>
			<month>{{ issueXml.issue_date|date:"m"}}</month>
			<year>{{ issueXml.issue_date|date:"Y" }}</year>
		</pub-date>
		<pub-date pub-type="actual_ppub">
			<day>{{ issueXml.issue_date|date:"d"}}</day>
			<month>{{ issueXml.issue_date|date:"m"}}</month>
			<year>{{ issueXml.issue_date|date:"Y" }}</year>
		</pub-date>
		<volume content-type="open-issue">{{ issueXml.volume }}</volume>
		<issue>{{ issueXml.issue }}</issue>
		<issue-id pub-id-type="publisher-id">{{ issueXml.publisher_id}}</issue-id>
	</issue-meta>
	<toc toc-type="page-order">
		{% for doi in issueXml.articles_dois %}
		<issue-article-meta>
			<article-id pub-id-type="doi">{{ doi}}</article-id>
		</issue-article-meta>
	   {% endfor %}
	</toc>
</issue-xml>
